program solve_basel;
const
	LOWERBOUND = 1;
	UPPERBOUND = 1000;
type
	double = real;
var
	a,b,i,sumval : integer;
	student_book : array [0..100] of student;

function sum ( b : double ) :  double;
	
	var
		i:integer;
		sum:double;
		term : array [LOWERBOUND..UPPERBOUND] of double;
	begin
		if b>=UPPERBOUND then
			b := UPPERBOUND ;
		i:=0;
		sum:=0;
		for i:= a to b do
		begin
			term[i] := 1.0/sqr(i);
			sum:=sum+i;
		end;
	end;


begin
	b := read();
	write ( sum(b) );
end
.
